    "def oldbingini(Y):\n",
    "    # for binary classification: p(-) = 1 - p(+)=>\n",
    "    # Gini = 2 * p(+) * (1 - p(+))\n",
    "    p = Y.sum() / len(Y)\n",
    "    return 2 * p * (1 - p)\n",
    "\n",
    "def oldbinentropy(Y):\n",
    "    # for binary classification: p(-) = 1 - p(+)=>\n",
    "    # Entropy = -p(+)log(p(+)) - (1 - p(+))log(1 - p(+))\n",
    "    p = Y.sum() / len(Y)\n",
    "    return -p*np.log(p) - (1 - p)*np.log(p)"
